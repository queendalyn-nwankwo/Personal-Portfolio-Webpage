    <script>
        const scriptURL = 'https://script.google.com/macros/s/AKfycbxneX-LyJ595uepjGGfdcYU3h6mhHZfT4_gMHDqkEJtCCIe6CI/exec'
        const form = document.forms['google-sheet']
      
        form.addEventListener('submit', e => {
          e.preventDefault()
          fetch(scriptURL, { method: 'POST', body: new FormData(form)})
            .then(response => alert("Thanks for Contacting me! I will get back to you soon..."))
            .catch(error => console.error('Error!', error.message))
        })
